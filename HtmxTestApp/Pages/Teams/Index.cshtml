@page
@using HtmxTestApp.Shared.Entities;
@model HtmxTestApp.Pages.Teams.IndexModel


<h4>Teams</h4>
<hr />
<table cellpadding="0" cellspacing="0">
    <tr>
        <th>CustomerID</th>
        <th>ContactName</th>
        <th>City</th>
        <th>Country</th>
    </tr>
    @foreach (Team team in Model.Teams)
    {
        <tr>
            <td>@team.Name</td>
            <td>Edit | Delete</td>
        </tr>
    }
</table>
