<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HtmxTestApp</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/HtmxTestApp.styles.css" asp-append-version="true" />
</head>
<body>
    <header style="border:solid black 1px">
        <nav>
            WBL
        </nav>
        <section>
            Scores
        </section>
    </header>
    <main role="main" style="border:solid black 1px">
        @RenderBody()
    </main>
    <aside>
        side panel stuff
    </aside>
    <footer>
        &copy; 2022 - HtmxTestApp - <a asp-area="" asp-page="/Privacy">Privacy</a>
    </footer>

    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>