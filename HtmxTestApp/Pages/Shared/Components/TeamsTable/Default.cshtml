@using HtmxTestApp.Shared.Entities;
@model IEnumerable<Team>


<table cellpadding="0" cellspacing="0">
    <tr>
        <th>
            Team
        </th>
        <th>
            Manage
        </th>
    </tr>
    @foreach (Team team in Model)
    {
        <tr>
            <td>@team.Name</td>
            <td><a asp-page="/Teams/Edit" asp-route-id="@team.Id" type="button" class="btn btn-edit btn-75w">Edit</a> | <a asp-page="/Teams/Edit" asp-route-id="@team.Id" type="button" class="btn btn-delete btn-75w">Delete</a></td>
        </tr>
    }
</table> 